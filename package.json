{
  "name": "ngx-signalr-websocket",
  "version": "1.2.0",
  "description": "A lightweight ASP.NET SignalR client for Angular",
  "license": "MIT",
  "keywords": [
    "angular",
    "signalr",
    "aspnetcore",
    "websocket",
    "rxjs"
  ],
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurivoronin/ngx-signalr-websocket.git"
  },
  "homepage": "https://github.com/yurivoronin/ngx-signalr-websocket/tree/main/projects/ngx-signalr-websocket#readme",
  "author": {
    "name": "Yuri Voronin"
  },
  "browserslist": "last 5 versions, iOS >= 12, Safari >= 12, not dead",
  "scripts": {
    "build": "parcel build",
    "test": "jest",
    "mock:build": "cd ./mock/SignalR.Example && docker build --tag signalr-example .",
    "mock:run": "docker run -d -p 8080:8080 --name signalr-example signalr-example",
    "mock:start": "docker start signalr-example",
    "mock:stop": "docker stop signalr-example",
    "mock:rm": "docker rm signalr-example",
    "mock:clear": "docker rm signalr-example && docker image rm signalr-example"
  },
  "dependencies": {
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.15.0",
    "@parcel/plugin": "^2.15.0",
    "@parcel/transformer-typescript-types": "^2.15.0",
    "@types/jest": "^30.0.0",
    "@types/node-fetch": "^2.6.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "node-fetch": "^2.7.0",
    "parcel": "^2.15.0",
    "parcel-resolver-ts-base-url": "^1.3.0",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  }
}
